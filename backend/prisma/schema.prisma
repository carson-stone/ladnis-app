// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id
  balance    Decimal
  credit     Int
  picture    String
  name_first String
  name_last  String
  employer   String
  email      String   @unique
  phone      BigInt
  address    String
  comments   String
  created    DateTime
  tags       String[]
}
